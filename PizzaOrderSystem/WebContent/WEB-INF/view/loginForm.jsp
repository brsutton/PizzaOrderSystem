<h2>Login Form</h2>

<form action="UserLoginController" method="post">
	<table border="0" cellpadding="5" cellspacing="3">
		<tr>
			<td>Login Name</td>
			<td><input type="text" name="loginName" /></td>
		</tr>
		<tr>
			<td>Password</td>
			<td><input type="password" name="password" /></td>
		</tr>
		<tr>
			<td colspan="2" style="text-align: right"><a href="regForm">Sign
					Up</a> <input type="submit" name="Login" value="Login" /></td>
		</tr>
	</table>
</form>